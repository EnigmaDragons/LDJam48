      - {fileID: 11400000, guid: 4a686078bcb00974895e62444b07026c, type: 2}
      followups:
      - speakingCharacter: {fileID: 11400000, guid: 92732e066aa377042a0b03744b773b65,
          type: 2}
        statement: I'll escort you to the exit shortly.
        statementEmotion:
          UseConstant: 0
          ConstantValue: 
          Variable: {fileID: 0}
        onlyShowIfCharacterLikedAnswer: 1
      - speakingCharacter: {fileID: 11400000, guid: 92732e066aa377042a0b03744b773b65,
          type: 2}
        statement: Imagine a worker bee that can't leave his own hive. No I couldn't
          imagine it either.
        statementEmotion:
          UseConstant: 0
          ConstantValue: 
          Variable: {fileID: 0}
        onlyShowIfCharacterLikedAnswer: 0
  - speakingCharacter: {fileID: 11400000, guid: 7fc050cb7ca6b9d428749a73e17c6e1c,
