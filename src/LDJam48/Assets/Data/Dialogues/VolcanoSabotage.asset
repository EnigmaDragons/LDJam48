      - {fileID: 11400000, guid: 486e9efc3d6b28c4cab97a366c02e466, type: 2}
      followups:
      - speakingCharacter: {fileID: 11400000, guid: 7fc050cb7ca6b9d428749a73e17c6e1c,
          type: 2}
        statement: That's my job.
        statementEmotion:
          UseConstant: 0
          ConstantValue: 
          Variable: {fileID: 0}
        onlyShowIfCharacterLikedAnswer: 0
      - speakingCharacter: {fileID: 11400000, guid: 92732e066aa377042a0b03744b773b65,
          type: 2}
        statement: 'We will catch them like a bird in a hang glider. '
        statementEmotion:
          UseConstant: 0
          ConstantValue: 
          Variable: {fileID: 0}
        onlyShowIfCharacterLikedAnswer: 1
      - speakingCharacter: {fileID: 11400000, guid: 92732e066aa377042a0b03744b773b65,
          type: 2}
        statement: Idiotic, you'd be dead if you found them.
        statementEmotion:
          UseConstant: 0
          ConstantValue: 
          Variable: {fileID: 0}
        onlyShowIfCharacterLikedAnswer: 0
    - text: This place is a maze and I have been trying to leave for the past 30
