      - {fileID: 11400000, guid: d3831e72a8e8d5442bf817d11fe17dbf, type: 2}
      followups:
      - speakingCharacter: {fileID: 11400000, guid: 17008e72f3df4cd4a83d031ad64f1518,
          type: 2}
        statement: Good thinking minion.
        statementEmotion:
          UseConstant: 0
          ConstantValue: 
          Variable: {fileID: 11400000, guid: d0d03c219bf579341b08c8849e01d6da, type: 2}
        onlyShowIfCharacterLikedAnswer: 1
      - speakingCharacter: {fileID: 11400000, guid: 17008e72f3df4cd4a83d031ad64f1518,
          type: 2}
        statement: <style="Loud"><style="Emphasize">HA!</style></style> You take
