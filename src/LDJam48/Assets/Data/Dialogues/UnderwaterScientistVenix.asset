      tags:
      - {fileID: 11400000, guid: 30cd045069badd146abc133d2b352ae7, type: 2}
      followups:
      - speakingCharacter: {fileID: 11400000, guid: 2dd26c43de20a094980c894d50d7ac0a,
          type: 2}
